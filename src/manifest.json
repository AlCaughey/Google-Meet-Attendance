{
    "name": "Google Meet Attendance",
    "description": "Allows you to input a list of invitees and then register who attended a Google Meet session",
    "permissions": ["storage"],
    "version": "0.1.2",
    "content_scripts": [
		{
			"matches": [
				"https://meet.google.com/*"
			],
			"css": [
				"css/jquery-ui.min.css",
				"css/styles.css"
			],
			"js": [
			  "js/jquery-3.5.0.min.js",
			  "js/jquery-ui.min.js",
			  "js/attendance.js"
			],
			"run_at": "document_idle",
			"all_frames": false
		}
    ],
    "page_action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		},
		"default_title": "Google Meet Attendance",
		"default_popup": "html/popup.html"
    },
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
    "manifest_version": 2
  }